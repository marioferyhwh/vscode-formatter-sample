Class MyApp.PatientManager Extends %Persistent
{


Property Name As %String;


Method SetName(pName As %String) {

    Set ..Name =pName;

}

Method GetName() As %String {
       Quit ..Name;
}

Method Save() As %Status {
  Try {
        Set sc=..%Save();
    }        Catch (Exception e) {
        Set sc = e.AsStatus();
    }
    Quit sc;
}

