{
    "name": "formatter-any",
    "displayName": "formatter-any",
    "description": "formay any language",
    "version": "0.0.1",
    "publisher": "mfrobayo",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:any-lang"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "any-lang",
                "aliases": [
                    "Foo"
                ],
                "extensions": [
                    "foo"
                ]
            }
        ],
        "commands": [
            {
                "command": "extension.format-any",
                "title": "Format any Files"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && tsc"
    },
    "dependencies": {
        "@types/node": "^20.14.2",
        "typescript": "^5.4.5",
        "vscode": "^1.1.37"
    }
}
